{
  "dashboard": {
    "title": "Brain Graph Overview",
    "panels": [
      {
        "title": "API Request Rate",
        "targets": [
          {
            "expr": "rate(traefik_service_requests_total{service=\"backend\"}[5m])"
          }
        ]
      },
      {
        "title": "Encoder Response Times",
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(http_request_duration_seconds_bucket[5m]))"
          }
        ]
      }
    ]
  }
}
